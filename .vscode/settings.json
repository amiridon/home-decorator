{
    "dotnet.defaultSolution": "HomeDecorator.sln",
    "[csharp]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-dotnettools.csharp"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "files.exclude": {
        "**/bin": true,
        "**/obj": true
    }
}
